{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:vue/essential",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "json"
    ],
    "rules": {
        "vue/multi-word-component-names": [
            "error",
            {
                "ignores": [
                    "login"
                ]
            }
        ]
    }
}
